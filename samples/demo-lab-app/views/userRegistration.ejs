<div class="container h-100">
    <div class="d-flex align-items-center justify-content-center">
      <div class="row position-absolute bottom-50">
        <form class="green p-5" method="POST" action="/user-registration/<%= did%>">
          <div class="mb-3">
            <p><b> Name:</b> <%= givenNames %></p>
            <p><b> Last Name:</b> <%= lastName %></p>
            <p><b> Passport Number:</b> <%= number %></p>
            <label for="test_sample" class="form-label">Test Sample Id</label>
            <input type="text" class="form-control green" id="test_sample" name="test_sample">
          </div>
          <button type="submit" class="btn btn-primary btn-lg green_btn start-50">
            Register a user
        </button>
         </form>
      </div>
    </div>
</div>